{
  "name": "template-web-component-react",
  "private": true,
  "version": "0.4.0",
  "license": "MIT",
  "packageManager": "pnpm@8.6.3",
  "scripts": {
    "dev": "pnpm storybook:dev",
    "build": "pnpm storybook:build",
    "test": "pnpm storybook:test",
    "test:ci": "pnpm storybook:test:ci",
    "storybook:dev": "storybook dev",
    "storybook:build": "storybook build",
    "storybook:server": "npx http-server storybook-static --port 6006 --silent",
    "storybook:test": "test-storybook --url http://127.0.0.1:6006",
    "storybook:test:ci": "start-server-and-test \"pnpm storybook:server\" http://127.0.0.1:6006 \"pnpm storybook:test\"",
    "lint": "eslint ./src ./.storybook --ext .js,.ts,.tsx",
    "i18n": "pnpm i18n:extract && pnpm i18n:compile",
    "i18n:extract": "formatjs extract src/**/*.ts* --ignore=**/*.d.ts --out-file locale/extracted/en.json --id-interpolation-pattern [sha512:contenthash:base64:6] --flatten",
    "i18n:compile": "formatjs compile-folder --ast locale/extracted locale/compiled"
  },
  "dependencies": {
    "@fontsource/rubik": "4.5.14",
    "@mui/icons-material": "5.11.16",
    "@mui/lab": "5.0.0-alpha.130",
    "@mui/material": "5.13.1",
    "immer": "10.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.4"
  },
  "devDependencies": {
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@storybook/addon-a11y": "7.0.20",
    "@storybook/addon-actions": "7.0.20",
    "@storybook/addon-docs": "7.0.20",
    "@storybook/addon-essentials": "7.0.20",
    "@storybook/addon-interactions": "7.0.24",
    "@storybook/addon-styling": "1.0.8",
    "@storybook/addon-viewport": "7.0.20",
    "@storybook/jest": "0.1.0",
    "@storybook/react": "7.0.20",
    "@storybook/react-vite": "7.0.20",
    "@storybook/storybook-deployer": "2.8.16",
    "@storybook/test-runner": "0.11.0",
    "@storybook/testing-library": "0.2.0",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "@vitejs/plugin-react": "4.0.0",
    "@vitejs/plugin-react-swc": "3.3.1",
    "cross-env": "7.0.3",
    "emotion-theming": "11.0.0",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-formatjs": "4.10.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-storybook": "0.6.12",
    "http-server": "14.1.1",
    "playwright": "1.35.1",
    "prettier": "2.8.8",
    "start-server-and-test": "2.0.0",
    "storybook": "7.0.20",
    "typescript": "5.0.4",
    "vite": "4.3.7",
    "vite-tsconfig-paths": "4.2.0"
  }
}
